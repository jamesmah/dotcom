  "dependencies": {
    "@fontsource/inter": "4.5.0",
    "aos": "2.3.4",
    "chroma-js": "2.1.2",
    "classnames": "2.3.1",
    "customize-cra": "1.0.0",
    "hamburgers": "1.1.3",
    "modern-normalize": "1.1.0",
