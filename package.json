    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "gh-pages": "3.2.3",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-prune": "0.10.0",
    "typescript": "4.1.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  }
